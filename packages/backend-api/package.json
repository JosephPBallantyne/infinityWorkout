{
  "name": "template",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "pre-build": "npm run lint & npm run clean",
    "post-build": "npm run copy-asset",
    "build": "npm run pre-build && npm run build-ts",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "NODE_ENV=local npm run serve"
  },
  "dependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "^2.8.9",
    "@types/hpp": "^0.2.1",
    "@types/morgan": "^1.9.2",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.22.3",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
